import numpy

N, M, _ = [int(x) for x in input().split()]
arr1 = []
arr2 = []

for _ in range(N):
    arr1.append([int(x) for x in input().split()])
for _ in range(M):
    arr2.append([int(x) for x in input().split()])
arr1 = numpy.array(arr1)
arr2 = numpy.array(arr2)
print(numpy.concatenate((arr1,arr2)))
